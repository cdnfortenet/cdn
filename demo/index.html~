        $(function () {
            try {
                var current = 0;
                $(".nav_out,.nav_over").attr("class", "nav_out").eq(current).attr("class", "nav_over");
            } catch (e) {
            }
            $("#nav li").hover(function () {
                $(this).find("p").slideDown(300);
            }, function () {
                $(this).find("p").hide();
